import pyautogui
import time

pyautogui.PAUSE = 0.3

pyautogui.press("win")
pyautogui.write("chrome")
pyautogui.press("enter")

time.sleep(3)
pyautogui.write("https://aplicacao.mpmg.mp.br/proconbloqueio/flogin")
pyautogui.press("enter")
time.sleep(3)
